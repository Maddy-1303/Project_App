<p style="color: green"><%= notice %></p>

<h1>Projects</h1>

<%= turbo_stream_from "projects" %>

<%= turbo_frame_tag "projects" do %>
  <%= render "form", project: @project %>
<% end %>

<% if current_user.admin? %>
  <%= turbo_frame_tag "projects" do %>
    <%= render @projects %>
  <% end %>
<% else %>
  <%= turbo_frame_tag "projects" do %>
    <%= render @projects %>

  <% end %>
<% end %>
